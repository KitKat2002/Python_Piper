#Creates the parent class
class Staff:
    Name: " "
    Phone: " "
    Age: " "
    Gender: " "
    Ethnicity: " "
    Languages_Spoken: " "
    Experience: " "
#Creates child class one
class Host/Hostess(Staff):
    People_Seated: " "
    Pay: " "
#Creates child class two
class Waiter/Waitress(Staff):
    Orders_Served: " "
    Orders_Taken_Back: " " 
